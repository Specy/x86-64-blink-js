{
  "name": "@specy/x86-64",
  "version": "1.0.4",
  "description": "An emulator for x86-64 based off https://x64.halb.it",
  "homepage": "https://github.com/Specy/x86-64-blink-js#readme",
  "bugs": {
    "url": "https://github.com/Specy/x86-64-blink-js/issues"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Specy/x86-64-blink-js.git"
  },
  "license": "ISC",
  "author": "specy",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "init": "npm run build:init-blink",
    "build:init-blink": "cd x86-64-playground && ./init_blink.sh",
    "build:blink": "cd x86-64-playground && ./compile_blink.sh && cp ./webapp/src/assets/blinkenlib.wasm ../src/deps/blinkenlib.wasm && cp ./webapp/src/assets/blinkenlib.js ../src/deps/blinkenlib.js",
    "build:binutils": "cd x86-64-playground && ./compile_musl_binutils.sh && cp ./webapp/src/assets/assemblers/gnu-as.elf ../src/deps/gnu-as.elf && cp ./webapp/src/assets/assemblers/gnu-ld.elf ../src/deps/gnu-ld.elf",
    "build:nasm": "cd x86-64-playground && ./compile_musl_nasm.sh && cp ./webapp/src/assets/assemblers/nasm.elf ../src/deps/nasm.elf",
    "build:deps": "concurrently \"npm run build:blink\" \"npm run build:binutils\" \"npm run build:nasm\" && npm run move-deps",
    "build:lib": "tsdown",
    "start": "npx tsx ./src/test.ts"
  },
  "devDependencies": {
    "@types/node": "^24.9.1",
    "concurrently": "^9.2.1",
    "tsdown": "^0.15.9",
    "typescript": "^5.9.3"
  }
}
